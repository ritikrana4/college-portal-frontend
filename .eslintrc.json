{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "plugin:react/recommended",
      "airbnb"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
      "react/react-in-jsx-scope" : "off",
      "react/jsx-filename-extension": [
          0,
          {
              "extensions": [
                  ".js",
                  ".jsx",
                  ".ts",
                  ".tsx"
              ]
          }
      ],
      "react/jsx-props-no-spreading": "off",
      "react/prop-types": "off",
      "no-shadow": "off"
  }
}
